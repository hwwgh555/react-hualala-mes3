const validateObject = {
    goodsCode: false,
    planProduceNum: false,
}
validateObject.goodsCode = !(modalManualAddTableRecords.some(record => {
    return (record.goodsCode === null || record.goodsCode === '')
}))
validateObject.planProduceNum = !(modalManualAddTableRecords.some(record => {
    return (record.planProduceNum === undefined || record.planProduceNum === null || record.planProduceNum === '')
}))
if (!validateObject.goodsCode) {
    message.warning('请选择品项!')
    return
}
if (!validateObject.planProduceNum) {
    message.warning('请输入数量!')
    return
}
